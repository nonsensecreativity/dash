{
    "name": "nonsensecreativity/dash",
    "description": "Kind of like LoDash but in PHP and lighter",
	"type": "library",
	"version": "0.0.1",
    "license": "MIT",
    "authors": [
        {
            "name": "Hermanto Lim",
            "email": "nonsensecreativity@gmail.com"
        }
    ],
	"require": {
		"php": ">=7.1"
	},
	"autoload": {
		"psr-4": {
			"NSC\\Dash\\": [
				"src/Dash",
				"src/Dash/lib"
			]
		}
	},
	"autoload-dev": {
		"psr-4": {
			"NSC\\Dash\\Test\\": "tests"
		}
	},
    "require-dev": {
        "phpunit/phpunit": "^6.3",
        "atoum/atoum": "^3.1",
        "atoum/stubs": "^2.5",
        "phpmd/phpmd": "^2.6"
    }
}
