{
    "name": "nonsensecreativity/dash",
    "description": "Kind of like LoDash but in PHP and lighter",
	"type": "library",
	"version": "0.0.1",
    "license": "MIT",
    "authors": [
        {
            "name": "Hermanto Lim",
            "email": "nonsensecreativity@gmail.com"
        }
    ],
	"require": {
		"php": ">=7.1"
	},
	"autoload": {
		"psr-4": {
			"NSC\\Dash\\": "src/Dash"
		},
		"files": [
			"src/Dash/lib/Compositions.php",
			"src/Dash/lib/Conditions.php",
    		"src/Dash/lib/DateTime.php",
    		"src/Dash/lib/Iterables.php",
    		"src/Dash/lib/Numbers.php",
    		"src/Dash/lib/Strings.php",
    		"src/Dash/lib/Universal.php"
    	]
	},
    "require-dev": {
        "phpunit/phpunit": "^6.3"
    }
}
